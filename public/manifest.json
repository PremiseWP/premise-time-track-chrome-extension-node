{
  "manifest_version": 2,

  "name": "Premise Time Tracker",
  "description": "Control the Premise Time Tracker Wordpress plugin right from your browser.",
  "version": "2.0",

  "browser_action": {
    "default_icon": "favicon.ico",
    "icons": ["favicon.ico"],
    "default_popup": "index.html",
    "default_title": "Start tracking time!"
  },
  "permissions": [
    "storage",
    "tabs",
    "*://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": ["land.html", "js/land.js"]
}
